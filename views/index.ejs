    <audio id="chordB" src="audio/chord/B.mp3" type="audio/mp3"></audio>

    <audio id="chordCm" src="audio/chord/Cm.mp3" type="audio/mp3"></audio>
    <audio id="chordDbm" src="audio/chord/Dbm.mp3" type="audio/mp3"></audio>
    <audio id="chordDm" src="audio/chord/Dm.mp3" type="audio/mp3"></audio>
