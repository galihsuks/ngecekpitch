<body>
    <header>
        <nav class="navbar">
            <div class="namaApp">AppChord</div>
            <input type="checkbox" id="check">
            <label for="check">
                <i class="material-icons" id="btn">menu</i>
            </label>
            <ul>
                <li data-nav="langkah1" class="nav-langkah"><a>Langkah 1</a></li>
                <li data-nav="langkah2" class="nav-langkah"><a>Langkah 2</a></li>
                <li data-nav="langkah3" class="nav-langkah"><a>Langkah 3</a></li>
                <li data-nav="langkah4" class="nav-langkah"><a>Langkah 4</a></li>
                <li data-nav="langkah5" class="nav-langkah"><a>Langkah 5</a></li>
                <li data-nav="langkah6" class="nav-langkah"><a>Langkah 6</a></li>
                <li data-nav="langkah7" class="nav-langkah"><a>Langkah 7</a></li>
                <li data-nav="langkah8" class="nav-langkah"><a>Langkah 8</a></li>
                <li data-nav="langkah9" class="nav-langkah"><a>Langkah 9</a></li>
                <li data-nav="langkah10" class="nav-langkah"><a>Langkah 10</a></li>
                <li data-nav="langkah11" class="nav-langkah"><a>Langkah 11</a></li>
            </ul>
        </nav>
    </header>

    <audio id="noteC" src="audio/C.mp3" type="audio/mp3"></audio>
    <audio id="noteDb" src="audio/Db.mp3" type="audio/mp3"></audio>
    <audio id="noteD" src="audio/D.mp3" type="audio/mp3"></audio>
    <audio id="noteEb" src="audio/Eb.mp3" type="audio/mp3"></audio>
    <audio id="noteE" src="audio/E.mp3" type="audio/mp3"></audio>
