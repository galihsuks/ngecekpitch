    <audio id="noteGb" src="audio/Gb.mp3" type="audio/mp3"></audio>
    <audio id="noteG" src="audio/G.mp3" type="audio/mp3"></audio>
    <audio id="noteAb" src="audio/Ab.mp3" type="audio/mp3"></audio>
    <audio id="noteA" src="audio/A.mp3" type="audio/mp3"></audio>
    <audio id="noteBb" src="audio/Bb.mp3" type="audio/mp3"></audio>
    <audio id="noteB" src="audio/B.mp3" type="audio/mp3"></audio>
    
    <audio id="chordC" src="audio/chord/C.mp3" type="audio/mp3"></audio>
    <audio id="chordDb" src="audio/chord/Db.mp3" type="audio/mp3"></audio>
    <audio id="chordD" src="audio/chord/D.mp3" type="audio/mp3"></audio>
    <audio id="chordEb" src="audio/chord/Eb.mp3" type="audio/mp3"></audio>
    <audio id="chordE" src="audio/chord/E.mp3" type="audio/mp3"></audio>
